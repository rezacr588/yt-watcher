# YouTube Watcher Configuration

# Video Settings
VIDEO_URL=https://www.youtube.com/watch?v=R71Rh1HR7Js

# Browser Settings
NUM_BROWSERS=10
HEADLESS=false

# Watch Time Settings (percentage of video to watch)
MIN_WATCH_PERCENTAGE=0.7
MAX_WATCH_PERCENTAGE=1.0

# Timing Settings (in seconds)
LAUNCH_DELAY_MIN=2
LAUNCH_DELAY_MAX=8
ACTION_DELAY_MIN=1
ACTION_DELAY_MAX=3

# Proxy Settings
USE_PROXY=false
PROXY_LIST_FILE=proxies.txt
PROXY_ROTATION=random  # Options: random, round-robin, sticky

# Behavior Settings
ENABLE_MOUSE_MOVEMENT=true
ENABLE_SCROLLING=true
ENABLE_VOLUME_CHANGES=true
ENABLE_QUALITY_CHANGES=false
ENABLE_FULLSCREEN=false

# Interaction Probabilities (0.0 to 1.0)
MOUSE_MOVEMENT_PROBABILITY=0.4
SCROLL_PROBABILITY=0.3
VOLUME_CHANGE_PROBABILITY=0.1
PAUSE_RESUME_PROBABILITY=0.05
SEEK_PROBABILITY=0.1

# Anti-Detection Settings
ROTATE_USER_AGENTS=true
RANDOM_LOCATIONS=true
STEALTH_MODE=true
RANDOMIZE_VIEWPORT=true

# Retry Settings
MAX_RETRIES=3
RETRY_DELAY=5

# Logging
LOG_LEVEL=INFO
LOG_FILE=youtube_watcher.log
